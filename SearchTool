    private void searhbuttonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_searhbuttonActionPerformed
               
                if(boxsearch1.getText()==null&&boxsearch2.getText()==null)
                {
                    JOptionPane.showMessageDialog(null,"Please enter url in box1 and 2");
                }
                else if(boxsearch1.getText()!=null&&boxsearch2.getText()!=null)
                {
                    ThreadSearch one = new ThreadSearch("Threadone",boxsearch1.getText(),showcontent1,showstatus1);
                    one.start();
                    ThreadSearch two = new ThreadSearch("Threadtwo",boxsearch2.getText(),showcontent2,showstatus2);
                    two.start();
                }
                else
                {
                    if(boxsearch1.getText()!=null)
                    {
                        
                         ThreadSearch two = new ThreadSearch("Threadtwo",boxsearch2.getText(),showcontent2,showstatus2);
                    two.start();
                    }
                    else
                    {
                   
                        ThreadSearch one = new ThreadSearch("Threadone",boxsearch1.getText(),showcontent1,showstatus1);
                    one.start();
                    }
                    
                }
                
                
    }//GEN-LAST:event_searhbuttonActionPerformed

